<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.street.mapper.ParameterMapper">
    <select id="queryValue" parameterType="String" resultType="String">
        select parameter_value from t_parameter where parameter_name = #{name}
    </select>
</mapper>

