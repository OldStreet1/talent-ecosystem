<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.street.mapper.EnterprideMapper">
    <insert id="addEnterpride" parameterType="com.street.dto.Enterprise">
        insert into t_enterprise (enterprise_name, enterprise_acc,enterprise_pwd,enterprise_address,enterprise_operation_status,enterprise_time_of_establishment,enterprise_legal_person,enterprise_code,enterprise_type,create_time)
        VALUE (#{enterprise_name},#{enterprise_acc},#{enterprise_pwd},#{enterprise_address},#{enterprise_operation_status},#{enterprise_time_establishment},#{enterprise_legal_person},#{enterprise_code},#{enterprise_type},#{create_time})
    </insert>

    <select id="queryEnterpride" parameterType="String" resultType="com.street.dto.Enterprise">
        select *
        from t_enterprise
        where enterprise_acc = #{enterprise_acc}
        ;
    </select>

    <select id="enterprideLogin" parameterType="com.street.dto.Enterprise" resultType="com.street.dto.Enterprise">
        select *
        from t_enterprise
        where enterprise_acc = #{enterprise_acc} and enterprise_pwd = #{enterprise_pwd}
        ;
    </select>
</mapper>
